{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2015","es2016", "esnext", "dom", "es5", "scripthost"],
    "noUnusedLocals": true
  },
  "include": [
    "src/**/*"
  ]
}
