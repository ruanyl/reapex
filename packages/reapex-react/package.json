{
  "name": "reapex-react",
  "version": "1.1.1",
  "description": "reapex react binding",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc --module es2015 --outDir lib/esm --declaration",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs --declaration",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@jest/types": "^28.0.0",
    "@types/jest": "^28.1.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "ajv": "~8.11.0",
    "eslint": "^8.19.0",
    "eslint-config-react-typescript": "^1.0.10",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^28.0.6",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "reapex": "workspace:^1.1.1",
    "use-sync-external-store": "^1.2.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "as"
    }
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/reapex.git"
  },
  "homepage": "https://github.com/ruanyl/reapex",
  "bugs": "https://github.com/ruanyl/reapex/issues"
}
