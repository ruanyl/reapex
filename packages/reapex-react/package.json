{
  "name": "reapex-react",
  "version": "1.0.0-beta.27",
  "description": "reapex react binding",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "scripts": {
    "build": "yarn run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2015 --outDir lib/esm --declaration",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs --declaration",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "ajv": "~8.6.3",
    "eslint": "^8.1.0",
    "eslint-config-react-typescript": "^1.0.7",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-as": "^1.3.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "reapex": "^1.0.0-beta.27",
    "use-sync-external-store": "^1.0.0-rc.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "as"
    }
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/reapex.git"
  },
  "homepage": "https://github.com/ruanyl/reapex",
  "bugs": "https://github.com/ruanyl/reapex/issues"
}
