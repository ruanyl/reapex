{
  "name": "vue-counter-example",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --open --skip-plugins @vue/cli-plugin-eslint",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.23.4",
    "reapex": "workspace:^1.1.1",
    "reapex-vue": "workspace:^1.1.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.19.0",
    "eslint-plugin-vue": "^9.2.0",
    "typescript": "^4.7.4"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "as"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "ignorePatterns": [
      "**/packages/*"
    ],
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
