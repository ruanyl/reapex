    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "ajv": "~8.5.0",
    "eslint": "^7.27.0",
    "eslint-config-react-typescript": "^1.0.7",
    "glob": "~7.1.7",
    "immutable": "^4.0.0-rc.12",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-as": "^1.2.3",
    "jest": "^27.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-jest": "^27.0.1",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "tapable": "^2.2.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
