{
  "name": "reapex",
  "version": "1.0.0-beta.17",
  "description": "A framework to build React application",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "yarn build && yarn build:vendor && webpack serve --config-name app --open",
    "build": "./node_modules/.bin/tsc --outDir lib --declaration",
    "build:vendor": "webpack --config webpack.config.js --config-name vendor",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.12",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "ajv": "~7.2.1",
    "eslint": "^7.22.0",
    "eslint-config-react-typescript": "^1.0.5",
    "glob": "~7.1.6",
    "html-webpack-plugin": "~5.3.1",
    "immutable": "^4.0.0-rc.12",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-as": "^1.2.3",
    "jest": "^26.6.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "~5.2.0",
    "react-router-dom": "~5.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "reselect": "~4.0.0",
    "ts-jest": "^26.5.3",
    "ts-loader": "~8.0.18",
    "typescript": "4.2",
    "webpack": "~5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "as"
    }
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/reapex.git"
  },
  "homepage": "https://github.com/ruanyl/reapex",
  "bugs": "https://github.com/ruanyl/reapex/issues"
}
